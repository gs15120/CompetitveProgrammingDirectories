bool as(P a,P b)
{
    if(a.x!=b.x)
    return a.x<b.x;
    return a.y>b.y;
}


int main()
{
    scanf("%d %d",&a,&b);
    a*=2;
    for(int t=1;t<=a;t++)
        scanf("%d",&o[t]);
    for(int t=1;t<=b;t++)
    {
        scanf("%d %d",&n,&m);
        l[n]=m;
        l[m]=n;
        if(o[n]>o[m])
        q.push({n,m});
        else q.push({m,n});
    }
    n=0;
    scanf("%d",&i);
    for(int t=0;t<a;t++)
    {
        if((t+i)%2)
        {
            //puts("!");
        if(l[n]==0||(j[l[n]]==1))
        {
            for(;q.size();q.pop())
            {
                if(j[q.front().x]==0)
                {
                    j[q.front().x]=1;
                    n=q.front().x;
                    printf("%d\n",n);
                    fflush(stdout);
                    q.pop();
                    goto qwe;
                }
            }
            c=d=0;
            for(int t=1;t<=a;t++)
                if(j[t]==0&&c<o[t]) c=o[t],d=t;
            j[d]=1;
            printf("%d\n",d);
            fflush(stdout);
            qwe:;
        }
        else
        {
            j[l[n]]=1;
            printf("%d\n",l[n]);
            fflush(stdout);
        }
        }
        else
        {
            scanf("%d",&n);
            j[n]=1;
        }
    }
}
